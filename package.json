{
  "name": "i18n-json-to-elm",
  "version": "1.3.1",
  "description": "Generates Elm sources from i18n's JSONs.",
  "main": "lib/index.js",
  "bin": {
    "i18n-json-to-elm": "bin/i18n-json-to-elm.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaackEng/i18n-json-to-elm.git"
  },
  "keywords": [
    "elm",
    "i18n"
  ],
  "author": "PaackEng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PaackEng/i18n-json-to-elm/issues"
  },
  "homepage": "https://github.com/PaackEng/i18n-json-to-elm#readme",
  "dependencies": {
    "elm-format": "^0.8.3"
  },
  "devDependencies": {
    "@types/node": "^18.11.8",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.14.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "prepare": "tsc",
    "lint": "prettier -c {bin,lib}/*.ts && eslint bin/*.ts lib/*.ts",
    "prettify": "prettier -w {bin,lib}/*.ts"
  },
  "files": [
    "bin/*.js",
    "lib/**/*.js"
  ]
}
